<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>VotingApp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
   <session-config>
  	<session-timeout>30</session-timeout>
  	<cookie-config>
  		<name>SID</name>
  	</cookie-config>
  	<tracking-mode>COOKIE</tracking-mode>
  </session-config>
  <context-param>
    	<param-name>apptitle</param-name>
    	<param-value>Online Voting System</param-value>
    </context-param>
</web-app>